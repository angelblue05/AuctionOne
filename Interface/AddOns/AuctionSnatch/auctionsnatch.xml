<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
..\..\FrameXML\UI.xsd"> 
	<Frame name="ASframe">
		<Scripts>
			<OnLoad>
				OnLoad(self);
			</OnLoad>
			<OnEvent>
				OnEvent(self, event);
			</OnEvent>
			<OnUpdate>
				OnUpdate(self, elapsed);
			</OnUpdate>
		</Scripts>
	</Frame>
</Ui> 